<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.ptc.com/schema/mvc"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/tool http://www.springframework.org/schema/tool/spring-tool-2.5.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
http://www.ptc.com/schema/mvc http://www.ptc.com/schema/mvc/mvc-10.0.xsd">

<bean id="xstreamFactoryBean" class="com.ptc.xworks.xmlobject.converter.XStreamFactoryBean">
	<property name="xmlObjectClassList">
		<bean class="com.ptc.xworks.util.TextFileListFactoryBean">
			<property name="configLocation" value="classpath:com/ptc/xworks/config/xmlobject.classes.conf" />
		</bean>
	</property>
</bean> 

<bean id="baseXstreamXmlObjectConverter" class="com.ptc.xworks.xmlobject.converter.BaseXstreamXmlObjectConverter">
	<property name="xstream" ref="xstreamFactoryBean" />
</bean>

<bean id="listBasedXmlObjectConverterResolver" class="com.ptc.xworks.xmlobject.converter.ListBasedXmlObjectConverterResolver">
	<property name="xmlObjectConverters">
		<list>
			<ref bean="baseXstreamXmlObjectConverter"/>
		</list>
	</property>
</bean>

<bean id="xmlObjectConverterManager" class="com.ptc.xworks.xmlobject.converter.DefaultXmlObjectConverterManager">
	<property name="converterResovler" ref="listBasedXmlObjectConverterResolver" />
</bean>

<bean id="threadLocalDatabaseResourceManager" class="com.ptc.xworks.xmlobject.store.oracle.ThreadLocalDbResourceManager">
	<property name="jdbcUrl" value="jdbc:oracle:thin:@wc102-m020.localvm.com:1521:WIND" />
	<property name="user" value="wind91" />
	<property name="password" value="wind91" />
</bean>

<bean id="windchillDatabaseResourceManager" class="com.ptc.xworks.xmlobject.windchill.WindchillDatabaseResourceManager">
</bean>

<bean id="databaseResourceManagerSelector" class="com.ptc.xworks.xmlobject.windchill.DatabaseResourceManagerSelector">
	<property name="localDatabaseResourceManager" ref="threadLocalDatabaseResourceManager" />
	<property name="windchillDatabaseResourceManager" ref="windchillDatabaseResourceManager" />
</bean>

<bean id="idGenerator" class="com.ptc.xworks.xmlobject.store.oracle.OracleSequenceIdGenerator">
	<property name="databaseResourceManager" ref="databaseResourceManagerSelector" />
	<property name="sequenceName" value="SEQ_XMLOBJECT_ID"></property>
</bean>

<bean id="defaultXmlObjectStoreAnnotationProcessor" class="com.ptc.xworks.xmlobject.annotation.processor.DefaultXmlObjectStoreAnnotationProcessor">
	<property name="databaseSchema" value=""/>
	<property name="overrideTableMap">
		<map>
			<entry key="com.ptc.china.gs.applicationform.RelatedObjectInfo" value="A_XML_RELATEDOBJECTINFO" />
		</map>
	</property>
</bean>
<bean id="baseOracleXmlObjectStoreMapper" class="com.ptc.xworks.xmlobject.store.oracle.BaseXmlObjectStoreMapper">
	<property name="databaseResourceManager" ref="databaseResourceManagerSelector" />
	<property name="xmlObjectConverterManager" ref="xmlObjectConverterManager" />
	<property name="currentUserResolver" ref="windchillCurrentUserResolver" />
	<property name="idGenerator" ref="idGenerator"/>
	<property name="xmlObjectStoreAnnotationProcessor" ref="defaultXmlObjectStoreAnnotationProcessor" />
	<property name="supportedClasses">
		<list>
			<value>com.ptc.xworks.xmlobject.BaseXmlObject</value>
		</list>
	</property>
</bean>

<bean id="baseXmlObjectLinkStoreMapper" class="com.ptc.xworks.xmlobject.store.oracle.BaseXmlObjectLinkStoreMapper" parent="baseOracleXmlObjectStoreMapper">
	<property name="throwExceptionIfNoDataInserted" value="false" />
	<property name="xmlObjectLinkClass" value="com.ptc.xworks.xmlobject.BaseXmlObjectLink" />
	<property name="supportedClasses">
		<list>
			<value>com.ptc.xworks.xmlobject.BaseXmlObjectLink</value>
		</list>
	</property>	
</bean>

<bean id="listBasedXmlObjectStoreMapperResolver" class="com.ptc.xworks.xmlobject.store.ListBasedXmlObjectStoreMapperResolver">
	<property name="mappers">
		<list>
			<ref bean="baseXmlObjectLinkStoreMapper"/>
			<ref bean="baseOracleXmlObjectStoreMapper"/>
		</list>
	</property>
</bean>

<bean id="mockCurrentUserResolver" class="com.ptc.xworks.xmlobject.store.oracle.MockCurrentUserResolver" />

<bean id="windchillCurrentUserResolver" class="com.ptc.xworks.xmlobject.windchill.WindchillCurrentUserResolver" />

<bean id="defaultXmlObjectLinkMarkerProcessor" class="com.ptc.xworks.xmlobject.store.DefaultXmlObjectLinkMarkerProcessor">
	<property name="xmlObjectStoreAnnotationProcessor" ref="defaultXmlObjectStoreAnnotationProcessor" />
</bean>

<bean id="defaultXmlObjectStoreManager" class="com.ptc.xworks.xmlobject.store.DefaultXmlObjectStoreManager">
	<property name="xmlObjectStoreMapperResolver" ref="listBasedXmlObjectStoreMapperResolver"/>
	<property name="currentUserResolver" ref="windchillCurrentUserResolver" />
	<property name="xmlObjectLinkStoreMapper" ref="baseXmlObjectLinkStoreMapper"/>
	<property name="xmlObjectStoreAnnotationProcessor" ref="defaultXmlObjectStoreAnnotationProcessor" />
	<property name="xmlObjectLinkMarkerProcessor" ref="defaultXmlObjectLinkMarkerProcessor" />
</bean>

<bean class="com.ptc.xworks.util.DependencyInjectionPostProcessor" >
	<property  name="storeManager" ref="defaultXmlObjectStoreManager" />
	<property name="messageSourceManager" ref="defaultClientMessageSourceManager" />
	<property name="webAnnotationProcess" ref="webAnnotationProcessor" />
	<property name="attributeGuiComponentBuilder" ref="defaultAttributeGuiComponentBuilder" />
	<property name="componentConfigFactory">
		<bean class="com.ptc.xworks.xmlobject.util.JcaComponentConfigFactoryResolver" />
	</property>
	<property name="currentLocaleResolver">
		<bean class="com.ptc.xworks.xmlobject.windchill.WindchillCurrentLocaleResolver" />
	</property>
	<property name="userLoalTimeZoneResolver">
		<bean class="com.ptc.xworks.xmlobject.windchill.WindchillUserLoalTimeZoneResolver"/>
	</property>
	<property name="xmlObjectSessionCache" ref="defaultXmlObjectSessionCache" />
	<property name="attributeDefinitionProvider" ref="defaultAttributeDefinitionProvider"/>
	<property name="globalReusableAttributeNamesResolver" ref="globalReusableAttributeNamesResolver" />
	<property name="attributePanelLayoutProvider" ref="javaAnnotationBasedAttributePanelLayoutProvider" />
</bean>

<bean id="windchillUserResolver" class="com.ptc.xworks.xmlobject.windchill.WindchillUserResolver" />

<bean id="dateConverter" class="com.ptc.xworks.xmlobject.attribute.converter.DateConverter"/>
<bean id="timestampConverter" class="com.ptc.xworks.xmlobject.attribute.converter.TimestampConverter"/>
<bean id="attachmentInfoConverter" class="com.ptc.xworks.xmlobject.attribute.converter.AttachmentInfoConverter"/>
<bean id="generatedHyperlinkConverter" class="com.ptc.xworks.xmlobject.attribute.converter.GeneratedHyperlinkConverter"/>
<bean id="windchillActionConverter" class="com.ptc.xworks.xmlobject.attribute.converter.WindchillActionConverter" />
<bean id="valueWithDisplayTextConverter" class="com.ptc.xworks.xmlobject.attribute.converter.ValueWithDisplayTextConverter"/>
<bean id="windchillObjectRefConverter" class="com.ptc.xworks.xmlobject.attribute.converter.WindchillObjectRefConverter">
	<property name="windchillObjectResolver" ref="windchillObjectResolver"/>
</bean>
<bean id="userInfoConverter" class="com.ptc.xworks.xmlobject.attribute.converter.UserInfoConverter">
	<property name="userResolver" ref="windchillUserResolver" />
</bean>
<bean id="classificationInfoConverter" class="com.ptc.xworks.xmlobject.attribute.converter.ClassificationInfoConverter"/>
<bean id="stringArrayConverter" class="com.ptc.xworks.xmlobject.attribute.converter.StringArrayConverter"/>
<bean id="floatingPointNumberWithUnitConverter" class="com.ptc.xworks.xmlobject.attribute.converter.FloatingPointNumberWithUnitConverter" />
<bean id="singleAttributeConverter" class="com.ptc.xworks.xmlobject.attribute.converter.SingleAttributeConverter">
	<property name="converters">
		<list>
			<ref bean="dateConverter"/>
			<ref bean="timestampConverter"/>
			<ref bean="attachmentInfoConverter"/>
			<ref bean="generatedHyperlinkConverter"/>
			<ref bean="userInfoConverter"/>
			<ref bean="windchillActionConverter"/>
			<ref bean="windchillObjectRefConverter" />
			<ref bean="valueWithDisplayTextConverter"/>
			<ref bean="classificationInfoConverter"/>
			<ref bean="floatingPointNumberWithUnitConverter" />
		</list>
	</property>
	<property name="supportedEnum">
		<bean class="com.ptc.xworks.util.TextFileListFactoryBean">
			<property name="configLocation" value="classpath:com/ptc/xworks/config/supportedEnum.conf" />
		</bean>
	</property>
</bean>
<bean id="arrayAttributeConverter" class="com.ptc.xworks.xmlobject.attribute.converter.ArrayAttributeConverter">
	<property name="converters">
		<list>
			<ref bean="singleAttributeConverter"/>
		</list>
	</property>
</bean>
<bean id="defaultAttributeConverter" class="com.ptc.xworks.xmlobject.attribute.converter.DefaultAttributeConverter">
	<property name="converters">
		<list>
			<ref bean="singleAttributeConverter"/>
			<ref bean="arrayAttributeConverter"/>
		</list>
	</property>
</bean>

<bean id="defaultWebFormExtractor" class="com.ptc.xworks.xmlobject.web.form.ValidatorSupportedWebFormExtractor">
	<property name="attributeDefinitionProvider" ref="defaultAttributeDefinitionProvider" />
</bean>

<bean id="typeBasedAttributeValueValidatorSelector" class="com.ptc.xworks.xmlobject.attribute.validator.TypeBasedAttributeValueValidatorSelector">

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE actionmodels SYSTEM "actionmodels.dtd">
<actionmodels>
    <model name="main navigation" id="browseActions" resourceBundle="com.ptc.core.ui.navigationRB">
        <description>
            Main navigation (tabs)
        </description>
        <action name="recentContexts" type="navigation"/>
        <action name="product" type="navigation"/>
        <action name="library" type="navigation"/>
        <action name="project" type="navigation"/>
        <action name="program" type="navigation"/>
        <action name="change" type="navigation"/>
        <action name="supplier" type="navigation"/>
        <action name="qms" type="navigation"/>
        <action name="org" type="navigation"/>
        <action name="site" type="navigation"/>
        <action name="clfAdmin" type="navigation"/>
        <action name="search" type="navigation"/>
        <!-- entry for customization tab -->
        <action name="customization" type="navigation"/>
        <action name="designconfig" type="navigation" />
        <!-- begin customize navigation tab-->
        <action name="controlPanel" type="navigation"/>
		<action name="rdm" type="navigation"/>
        <!-- end customize section navigation tab-->
    </model>
    <model name="controlPanel navigation" defaultActionName="controlPanel" defaultActionType="importOptimumSelectingLevel">
        <description>Sub tabs under the main tab</description>
		<action name="importOptimumSelectingLevel" type="controlPanel" />
		<action name="ecrReport" type="controlPanel" />
		<action name="processApprovalReport" type="controlPanel" />
		<action name="bomReleaseReport" type="controlPanel" />
		<action name="wfRejectReport" type="controlPanel" />
		<action name="inventoryDisposition" type="controlPanel" />
		<action name="notValidSuperstratum" type="controlPanel" />
		<action name="orgOrSiteGroupReport" type="controlPanel" />
		<action name="productRoleUserReport" type="controlPanel" />
		<action name="trunkBOMQueryReport" type="controlPanel" />
		<action name="productFolderReport" type="controlPanel" />
		<action name="changeReport" type="controlPanel" />
    </model>
    
    <model name="designconfig navigation" resourceBundle="com.catl.require.resource.requireRB">
		<description>
			ri tabs under the main tab
		</description>
		<action name="evcconfig" type="confignav" />
		<action name="riconfig" type="confignav" />
		<action name="pdconfig" type="confignav" />
	</model>
</actionmodels>
